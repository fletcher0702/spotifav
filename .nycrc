{
  "check-coverage": false,
  "per-file": true,
  "lines": 60,
  "statements": 60,
  "functions": 60,
  "branches": 60,
  "include": [
    "./src/**/*.js"
  ],
  "exclude":[
    "./src/**/*.spec.js"
  ],
  "reporter": [
    "lcov",
    "text-summary",
    "html"
  ],
  "require": [
    "babel-register"
  ],
  "all": true,
  "cache": false,
  "report-dir": "./coverage",
  "sourceMap": false,
  "instrument": false
}
